<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OperableItemView">
        <attr name="oiv_briefText" format="reference|string" />
        <attr name="oiv_bodyText" format="reference|string" />
        <attr name="oiv_drawablePadding" format="reference|dimension" />
        <attr name="oiv_textInterval" format="reference|dimension" />
        <attr name="oiv_briefTextSize" format="reference|dimension" />
        <attr name="oiv_bodyTextSize" format="reference|dimension" />
        <attr name="oiv_briefTextColor" format="reference|color" />
        <attr name="oiv_bodyTextColor" format="reference|color" />
        <attr name="oiv_dividerHeight" format="reference|dimension" />
        <attr name="oiv_endDrawable" format="reference" />
        <attr name="oiv_startDrawable" format="reference" />
        <attr name="oiv_dividerDrawable" format="reference" />
        <attr name="oiv_gravity">
            <flag name="left" value="0x01" />
            <flag name="top" value="0x02" />
            <flag name="right" value="0x04" />
            <flag name="bottom" value="0x08" />
            <flag name="center" value="0x30" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x20" />
        </attr>
        <attr name="oiv_shadowColor" format="reference|color" />
        <attr name="oiv_shadowRadius" format="reference|dimension" />
        <attr name="oiv_shadowDx" format="reference|dimension" />
        <attr name="oiv_shadowDy" format="reference|dimension" />
        <attr name="oiv_shadowSide">
            <flag name="left" value="0x0001" />
            <flag name="right" value="0x0010" />
            <flag name="top" value="0x0100" />
            <flag name="bottom" value="0x1000" />
            <flag name="all" value="0x1111" />
            <flag name="none" value="0" />
        </attr>
        <attr name="oiv_briefTextTypeface" format="string" />
        <attr name="oiv_bodyTextTypeface" format="string" />
        <attr name="oiv_briefDefaultText" format="string" />
        <attr name="oiv_bodyDefaultText" format="string" />
        <attr name="oiv_drawableChainStyle">
            <enum name="spread_inside" value="10" />
            <enum name="packed" value="20" />
        </attr>
        <attr name="oiv_drawableAlignStyle">
            <flag name="brief_start" value="0x01" />
            <flag name="body_start" value="0x02" />
            <flag name="brief_end" value="0x04" />
            <flag name="body_end" value="0x08" />
            <flag name="normal" value="0x10" />
        </attr>
        <attr name="oiv_startDrawableWidth" format="reference|dimension" />
        <attr name="oiv_startDrawableHeight" format="reference|dimension" />
        <attr name="oiv_endDrawableWidth" format="reference|dimension" />
        <attr name="oiv_endDrawableHeight" format="reference|dimension" />
    </declare-styleable>
</resources>